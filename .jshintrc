{
  "camelcase": true
  , "quotmark": "single"
  , "eqnull": true
  , "node": true
  , "browser": false
  , "expr": true
  , "indent": 2
  , "maxdepth": 5
  , "maxlen": 90
  , "maxstatements": 100
  , "eqeqeq": true
  , "undef": true
  , "unused": "vars"
  , "trailing": true
  , "white": false
  , "globals" : {
      "it": true
      , "console": true
      , "logger": true
      , "describe": true
      , "expect": true
      , "beforeEach": true
      , "afterEach": true
    }
}
